<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.abot.dynamicdb.mapper.CityMapper">

    <resultMap id="BaseResultMap" type="com.abot.dynamicdb.entity.City">
            <id property="id" column="ID" jdbcType="INTEGER"/>
            <result property="name" column="Name" jdbcType="CHAR"/>
            <result property="countrycode" column="CountryCode" jdbcType="CHAR"/>
            <result property="district" column="District" jdbcType="CHAR"/>
            <result property="population" column="Population" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        ID,Name,CountryCode,
        District,Population
    </sql>
</mapper>
